{
  "name": "generatic",
  "version": "1.0.0",
  "description": "Generate unique layer-based images by Nodejs for NFT",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node index.ts",
    "serve": "ts-node-dev --respawn index.ts"
  },
  "pkg": {
    "scripts": "src/**/*.js",
    "assets":["node_modules/canvas/build/Release/*", "node_modules/terminal-kit/**/*"]
